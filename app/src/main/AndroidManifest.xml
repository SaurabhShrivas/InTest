<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="saurabhshrivas.csenitrr.com.intest">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".InTest"
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".SignUp.Register" />
        <activity android:name=".Home.Home" />
        <activity android:name=".Discussion.Discuss" />
        <activity android:name=".Discussion.UserList"></activity>
    </application>

</manifest>